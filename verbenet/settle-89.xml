<?xml version="1.0" encoding="UTF-8"?>
<VNCLASS ID="settle-89" ladl="14 ou 16" lvf="U2a ou F4b ou X1a">
     <MEMBERS>
          <MEMBER name="accorder s'"/>
          <MEMBER name="allier s'"/>
          <MEMBER name="arranger s'"/>
          <MEMBER name="entendre s'"/>
     </MEMBERS>
     <THEMROLES>
          <THEMROLE type="Agent">
               <SELRESTRS logic="or">
                    <SELRESTR Value="+" type="organization"/>
                    <SELRESTR Value="+" type="animate"/>
               </SELRESTRS>
          </THEMROLE>
          <THEMROLE type="Co-Agent">
               <SELRESTRS logic="or">
                    <SELRESTR Value="+" type="organization"/>
                    <SELRESTR Value="+" type="animate"/>
               </SELRESTRS>
          </THEMROLE>
          <THEMROLE type="Goal"/>
     </THEMROLES>
     <FRAMES>
          <FRAME>
               <DESCRIPTION primary="NP V PP.co-agent pour V-inf"/>
               <EXAMPLES>
                    <EXAMPLE>Luc s'est entendu avec Marie pour proposer un projet</EXAMPLE>
               </EXAMPLES>
               <SYNTAX>
                    <NP value="Agent">
                         <SYNRESTRS/>
                    </NP>
                    <VERB/>
                    <PREP>
                         <SELRESTRS>
                              <SELRESTR Value="avec"/>
                         </SELRESTRS>
                    </PREP>
                    <NP value="Co-Agent">
                         <SYNRESTRS/>
                    </NP>
                    <PREP>
                         <SELRESTRS>
                              <SELRESTR Value="pour"/>
                         </SELRESTRS>
                    </PREP>
                    <VINF emptysubjectrole="Agent" value="Goal"/>
               </SYNTAX>
               <SEMANTICS>agree(result(E), Agent, Co-Agent, Goal)</SEMANTICS>
          </FRAME>
          <FRAME>
               <DESCRIPTION primary="NP V PP.co-agent PP"/>
               <EXAMPLES>
                    <EXAMPLE>Luc s'est entendu avec Marie sur le prix.</EXAMPLE>
               </EXAMPLES>
               <SYNTAX>
                    <NP value="Agent">
                         <SYNRESTRS/>
                    </NP>
                    <VERB/>
                    <PREP>
                         <SELRESTRS>
                              <SELRESTR Value="avec"/>
                         </SELRESTRS>
                    </PREP>
                    <NP value="Co-Agent">
                         <SYNRESTRS/>
                    </NP>
                    <PREP>
                         <SELRESTRS>
                              <SELRESTR Value="sur"/>
                         </SELRESTRS>
                    </PREP>
                    <NP value="Goal">
                         <SYNRESTRS/>
                    </NP>
               </SYNTAX>
               <SEMANTICS>agree(result(E), Agent, Co-Agent, Goal)</SEMANTICS>
          </FRAME>
          <FRAME>
               <DESCRIPTION primary="NP V pour V-inf"/>
               <EXAMPLES>
                    <EXAMPLE>Luc et Marie se sont entendus pour proposer un projet</EXAMPLE>
               </EXAMPLES>
               <SYNTAX>
                    <NP modifier="+plural" value="Agent">
                         <SYNRESTRS/>
                    </NP>
                    <VERB/>
                    <PREP>
                         <SELRESTRS>
                              <SELRESTR Value="pour"/>
                         </SELRESTRS>
                    </PREP>
                    <VINF emptysubjectrole="Agent" value="Goal"/>
               </SYNTAX>
               <SEMANTICS>agree(result(E), Agent, Goal)</SEMANTICS>
          </FRAME>
          <FRAME>
               <DESCRIPTION primary="NP V PP"/>
               <EXAMPLES>
                    <EXAMPLE>Luc et Marie se sont entendus sur le prix</EXAMPLE>
               </EXAMPLES>
               <SYNTAX>
                    <NP modifier="+plural" value="Agent">
                         <SYNRESTRS/>
                    </NP>
                    <VERB/>
                    <PREP>
                         <SELRESTRS>
                              <SELRESTR Value="sur"/>
                         </SELRESTRS>
                    </PREP>
                    <NP value="Goal">
                         <SYNRESTRS/>
                    </NP>
               </SYNTAX>
               <SEMANTICS>agree(result(E), Agent, Goal)</SEMANTICS>
          </FRAME>
     </FRAMES>
</VNCLASS>
