<?xml version="1.0" encoding="UTF-8"?>
<VNCLASS ID="see-30.1" ladl="6[+N1 =: si P ou si P]" lvf="H1d[-N1g]">
     <MEMBERS>
          <MEMBER name="flairer"/>
          <MEMBER name="regarder"/>
          <MEMBER name="palper"/>
          <MEMBER name="tâter"/>
          <MEMBER name="ouïr"/>
          <MEMBER name="entrevoir"/>
          <MEMBER name="reluquer"/>
          <MEMBER name="renifler"/>
          <MEMBER name="zieuter"/>
          <MEMBER name="contrôler"/>
          <MEMBER name="espionner"/>
          <MEMBER name="guetter"/>
          <MEMBER name="humer"/>
          <MEMBER name="investiguer"/>
          <MEMBER name="scruter"/>
          <MEMBER name="sonder"/>
          <MEMBER name="surveiller"/>
          <MEMBER name="vérifier"/>
          <MEMBER name="contempler"/>
          <MEMBER name="épier"/>
          <MEMBER name="lorgner"/>
          <MEMBER name="goûter"/>
          <MEMBER name="ressentir"/>
          <MEMBER name="discerner"/>
          <MEMBER name="écouter"/>
          <MEMBER name="percevoir"/>
     </MEMBERS>
     <THEMROLES>
          <THEMROLE type="Experiencer">
               <SELRESTR Value="+" type="animate"/>
          </THEMROLE>
          <THEMROLE type="Stimulus"/>
     </THEMROLES>
     <FRAMES>
          <FRAME>
               <DESCRIPTION primary="NP V NP"/>
               <EXAMPLES>
                    <EXAMPLE>J'ai vu le match</EXAMPLE>
               </EXAMPLES>
               <SYNTAX>
                    <NP value="Experiencer">
                         <SYNRESTRS/>
                    </NP>
                    <VERB/>
                    <NP value="Stimulus">
                         <SYNRESTRS/>
                    </NP>
               </SYNTAX>
               <SEMANTICS>perceive(during(E), Experiencer, Stimulus) in_reaction_to(E, Stimulus)</SEMANTICS>
          </FRAME>
          <FRAME>
               <DESCRIPTION primary="NP V si P"/>
               <EXAMPLES>
                    <EXAMPLE>J'ai vu s'il avait vidé les poubelles</EXAMPLE>
               </EXAMPLES>
               <SYNTAX>
                    <NP value="Experiencer">
                         <SYNRESTRS/>
                    </NP>
                    <VERB/>
                    <P introduced_by="si" value="Stimulus"/>
               </SYNTAX>
               <SEMANTICS>perceive(during(E), Experiencer, Stimulus) in_reaction_to(E, Stimulus)</SEMANTICS>
          </FRAME>
     </FRAMES>
     <SUBCLASSES>
          <VNSUBCLASS ID="30.1-1" ladl="6[+N1 =: Qu Pind]">
               <MEMBERS>
                    <MEMBER name="remarquer"/>
                    <MEMBER name="repérer"/>
                    <MEMBER name="reconnaître"/>
                    <MEMBER name="constater"/>
               </MEMBERS>
               <THEMROLES/>
               <FRAMES>
                    <FRAME>
                         <DESCRIPTION primary="NP V Qu Pind"/>
                         <EXAMPLES>
                              <EXAMPLE>Luc a vu qu'il pleuvait beaucoup.</EXAMPLE>
                         </EXAMPLES>
                         <SYNTAX>
                              <NP value="Experiencer">
                                   <SYNRESTRS/>
                              </NP>
                              <VERB/>
                              <PIND value="Stimulus"/>
                         </SYNTAX>
                         <SEMANTICS>perceive(during(E), Experiencer, Stimulus) in_reaction_to(E, Stimulus)</SEMANTICS>
                    </FRAME>
               </FRAMES>
               <SUBCLASSES>
                    <VNSUBCLASS ID="30.1-1-1" ladl="6[+N1 = (N) (V-inf W) ou +N1 = (N) (V-ant W)]">
                         <MEMBERS>
                              <MEMBER name="voir"/>
                              <MEMBER name="observer"/>
                              <MEMBER name="entendre"/>
                              <MEMBER name="apercevoir"/>
                              <MEMBER name="sentir"/>
                         </MEMBERS>
                         <THEMROLES/>
                         <FRAMES>
                              <FRAME>
                                   <DESCRIPTION primary="NP V NP V-inf"/>
                                   <EXAMPLES>
                                        <EXAMPLE>Luc a vu Marie traverser la rue.</EXAMPLE>
                                   </EXAMPLES>
                                   <SYNTAX>
                                        <NP value="Experiencer">
                                             <SYNRESTRS/>
                                        </NP>
                                        <VERB/>
                                        <VINF value="Stimulus"/>
                                   </SYNTAX>
                                   <SEMANTICS>perceive(during(E), Experiencer, Stimulus) in_reaction_to(E, Stimulus)</SEMANTICS>
                              </FRAME>
                              <FRAME>
                                   <DESCRIPTION primary="NP V NP V-ant"/>
                                   <EXAMPLES>
                                        <EXAMPLE>Luc a vu Marie traversant la rue.</EXAMPLE>
                                   </EXAMPLES>
                                   <SYNTAX>
                                        <NP value="Experiencer">
                                             <SYNRESTRS/>
                                        </NP>
                                        <VERB/>
                                        <VINF value="Stimulus"/>
                                   </SYNTAX>
                                   <SEMANTICS>perceive(during(E), Experiencer, Stimulus) in_reaction_to(E, Stimulus)</SEMANTICS>
                              </FRAME>
                         </FRAMES>
                    </VNSUBCLASS>
               </SUBCLASSES>
          </VNSUBCLASS>
     </SUBCLASSES>
</VNCLASS>
