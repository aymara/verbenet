<?xml version="1.0" encoding="UTF-8"?>
<VNCLASS ID="fulfilling-13.4.1">
     <MEMBERS>
          <MEMBER name="livrer"/>
          <MEMBER name="fournir"/>
          <MEMBER name="servir"/>
     </MEMBERS>
     <THEMROLES>
          <THEMROLE type="Agent">
               <SELRESTRS logic="or">
                    <SELRESTR Value="+" type="organization"/>
                    <SELRESTR Value="+" type="animate"/>
               </SELRESTRS>
          </THEMROLE>
          <THEMROLE type="Theme"/>
          <THEMROLE type="Recipient">
               <SELRESTRS logic="or">
                    <SELRESTR Value="+" type="organization"/>
                    <SELRESTR Value="+" type="animate"/>
               </SELRESTRS>
          </THEMROLE>
     </THEMROLES>
     <FRAMES>
          <FRAME>
               <DESCRIPTION primary="NP V NP PP.recipient"/>
               <EXAMPLES>
                    <EXAMPLE>Luc fournit du lait à Max</EXAMPLE>
               </EXAMPLES>
               <SYNTAX>
                    <NP value="Agent">
                         <SYNRESTRS/>
                    </NP>
                    <VERB/>
                    <NP value="Theme">
                         <SYNRESTRS/>
                    </NP>
                    <PREP>
                         <SELRESTRS>
                              <SELRESTR Value="à"/>
                         </SELRESTRS>
                    </PREP>
                    <NP value="Recipient">
                         <SYNRESTRS/>
                    </NP>
               </SYNTAX>
               <SEMANTICS>has_possession(start(E), Agent, Theme) has_possession(end(E), Recipient, Theme) transfer(during(E), Theme) cause(Agent, E)</SEMANTICS>
          </FRAME>
          <FRAME>
               <DESCRIPTION primary="NP V NP PP.theme"/>
               <EXAMPLES>
                    <EXAMPLE>Luc fournit Max de/en lait.</EXAMPLE>
               </EXAMPLES>
               <SYNTAX>
                    <NP value="Agent">
                         <SYNRESTRS/>
                    </NP>
                    <VERB/>
                    <NP value="Recipient">
                         <SYNRESTRS/>
                    </NP>
                    <PREP>
                         <SELRESTRS>
                              <SELRESTR Value="de;en"/>
                         </SELRESTRS>
                    </PREP>
                    <NP value="Theme">
                         <SYNRESTRS/>
                    </NP>
               </SYNTAX>
               <SEMANTICS>has_possession(start(E), Agent, Theme) has_possession(end(E), Recipient, Theme) transfer(during(E), Theme) cause(Agent, E)</SEMANTICS>
          </FRAME>
     </FRAMES>
</VNCLASS>
