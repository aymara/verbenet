     </THEMROLES>
     <FRAMES>
          <FRAME>
               <DESCRIPTION primary="NP V NP.Theme PP.attribute"/>
               <EXAMPLES>
                    <EXAMPLE>Ils ont loué les volontaires pour leur courage.</EXAMPLE>
               </EXAMPLES>
               <SYNTAX>
                    <NP value="Agent">
                         <SYNRESTRS/>
                    </NP>
                    <VERB/>
                    <NP value="Theme">
                         <SYNRESTRS/>
                    </NP>
                    <PREP>
                         <SELRESTRS>
                              <SELRESTR Value="pour"/>
                         </SELRESTRS>
                    </PREP>
                    <NP value="Attribute">
                         <SYNRESTRS/>
                    </NP>
               </SYNTAX>
               <SEMANTICS>judge(during(E), Agent, Theme, Attribute)</SEMANTICS>
          </FRAME>
          <FRAME>
               <DESCRIPTION primary="NP V NP.Attribute"/>
