                    </NP>
                    <PREP>
                         <SELRESTRS>
                              <SELRESTR Value="de;pour"/>
                         </SELRESTRS>
                    </PREP>
                    <NP modifier="+VTheme-inf" value="Attribute">
                         <SYNRESTRS/>
                    </NP>
               </SYNTAX>
               <SEMANTICS>judge(during(E), Agent, Theme, Attribute)</SEMANTICS>
          </FRAME>
