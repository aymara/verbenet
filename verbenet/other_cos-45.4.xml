     <FRAMES>
          <FRAME>
               <DESCRIPTION primary="NP V NP PP.instrument" syntax="Agent V Patient {avec} Instrument"/>
               <EXAMPLES>
                    <EXAMPLE>Luc a séché les draps avec ce séchoir à cheveux</EXAMPLE>
               </EXAMPLES>
               <SYNTAX>
                    <NP value="Agent">
                         <SYNRESTRS/>
                    </NP>
                    <VERB/>
                    <NP value="Patient">
                         <SYNRESTRS/>
                    </NP>
                    <PREP>
                         <SELRESTRS>
                              <SELRESTR Value="avec"/>
                         </SELRESTRS>
                    </PREP>
                    <NP value="Instrument">
                         <SYNRESTRS/>
                    </NP>
               </SYNTAX>
               <SEMANTICS>cause(Agent, E) state(result(E), Endstate, Patient) use(during(E), Agent, Instrument)</SEMANTICS>
          </FRAME>
          <FRAME>
               <DESCRIPTION primary="NP se V ADV" syntax="Patient se V&lt;+middle&gt; ADV"/>
               <EXAMPLES>
                    <EXAMPLE>Le coton se sèche facilement</EXAMPLE>
               </EXAMPLES>
               <SYNTAX>
                    <NP value="Patient">
                         <SYNRESTRS/>
                    </NP>
                    <VERB pronominal="true" restr="middle"/>
                    <ADV/>
               </SYNTAX>
               <SEMANTICS>property(Patient, Prop) Adv(Prop)</SEMANTICS>
          </FRAME>
          <FRAME>
               <DESCRIPTION primary="NP.instrument V NP" syntax="Instrument V Patient"/>
               <EXAMPLES>
                    <EXAMPLE>Ce séchoir à cheveux a séché les draps</EXAMPLE>
               </EXAMPLES>
               <SYNTAX>
                    <NP value="Instrument">
                         <SYNRESTRS/>
                    </NP>
                    <VERB/>
                    <NP value="Patient">
                         <SYNRESTRS/>
                    </NP>
               </SYNTAX>
               <SEMANTICS>use(during(E), ?Agent, Instrument) state(result(E), Endstate, Patient)</SEMANTICS>
          </FRAME>
     </FRAMES>
