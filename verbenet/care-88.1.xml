<?xml version="1.0" encoding="UTF-8"?>
<VNCLASS ID="care-88.1" ladl="8" lvf="P1a">
     <MEMBERS>
          <MEMBER name="branler se"/>
          <MEMBER name="contreficher se"/>
          <MEMBER name="contrefoutre se"/>
          <MEMBER name="ficher se"/>
          <MEMBER name="foutre se"/>
          <MEMBER name="inquiéter s'"/>
          <MEMBER name="préoccuper se"/>
          <MEMBER name="tourmenter se"/>
          <MEMBER name="tracasser se"/>
     </MEMBERS>
     <THEMROLES>
          <THEMROLE type="Experiencer">
               <SELRESTRS logic="or">
                    <SELRESTR Value="+" type="organization"/>
                    <SELRESTR Value="+" type="animate"/>
               </SELRESTRS>
          </THEMROLE>
          <THEMROLE type="Stimulus"/>
     </THEMROLES>
     <FRAMES>
          <FRAME>
               <DESCRIPTION primary="NP V de comment P" syntax="Experiencer V {de} Stimulus&lt;+comment P&gt;"/>
               <EXAMPLES>
                    <EXAMPLE>Luc se fiche de comment Marie vient</EXAMPLE>
               </EXAMPLES>
               <SYNTAX>
                    <NP value="Experiencer">
                         <SYNRESTRS/>
                    </NP>
                    <VERB/>
                    <PREP>
                         <SELRESTRS>
                              <SELRESTR Value="de"/>
                         </SELRESTRS>
                    </PREP>
                    <P introduced_by="comment" value="Stimulus"/>
               </SYNTAX>
               <SEMANTICS>emotional_state(result(E), Emotion, Experiencer) in_reaction_to(E, Stimulus)</SEMANTICS>
          </FRAME>
          <FRAME>
               <DESCRIPTION primary="NP V de ce Qu Psubj" syntax="Experiencer V {de} Stimulus&lt;+Qu Psubj&gt;"/>
               <EXAMPLES>
                    <EXAMPLE>Luc se fiche (de ce) que Léa soit morte</EXAMPLE>
               </EXAMPLES>
               <SYNTAX>
                    <NP value="Experiencer">
                         <SYNRESTRS/>
                    </NP>
                    <VERB/>
                    <PREP>
                         <SELRESTRS>
                              <SELRESTR Value="de"/>
                         </SELRESTRS>
                    </PREP>
                    <PSUBJ introduced_by="de" value="Stimulus"/>
               </SYNTAX>
               <SEMANTICS>emotional_state(result(E), Emotion, Experiencer) in_reaction_to(E, Stimulus)</SEMANTICS>
          </FRAME>
          <FRAME>
               <DESCRIPTION primary="NP V PP.stimulus" syntax="Experiencer V {de} Stimulus"/>
               <EXAMPLES>
                    <EXAMPLE>Luc se fiche de la santé de Marie</EXAMPLE>
               </EXAMPLES>
               <SYNTAX>
                    <NP value="Experiencer">
                         <SYNRESTRS/>
                    </NP>
                    <VERB/>
                    <PREP>
                         <SELRESTRS>
                              <SELRESTR Value="de"/>
                         </SELRESTRS>
                    </PREP>
                    <NP value="Stimulus">
                         <SYNRESTRS/>
                    </NP>
               </SYNTAX>
               <SEMANTICS>emotional_state(result(E), Emotion, Experiencer) in_reaction_to(E, Stimulus)</SEMANTICS>
          </FRAME>
          <FRAME>
               <DESCRIPTION primary="NP V de si P" syntax="Experiencer V {de} Stimulus&lt;+si P&gt;"/>
               <EXAMPLES>
                    <EXAMPLE>Luc se fiche de si Marie vient ou non</EXAMPLE>
               </EXAMPLES>
               <SYNTAX>
                    <NP value="Experiencer">
                         <SYNRESTRS/>
                    </NP>
                    <VERB/>
                    <PREP>
                         <SELRESTRS>
                              <SELRESTR Value="de"/>
                         </SELRESTRS>
                    </PREP>
                    <P introduced_by="si" value="Stimulus"/>
               </SYNTAX>
               <SEMANTICS>emotional_state(result(E), Emotion, Experiencer) in_reaction_to(E, Stimulus)</SEMANTICS>
          </FRAME>
     </FRAMES>
</VNCLASS>
