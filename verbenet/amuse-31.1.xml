     <FRAMES>
          <FRAME>
               <DESCRIPTION primary="NP V NP" syntax="Attribute&lt;+genitive (Stimulus)&gt; V Experiencer"/>
               <EXAMPLES>
                    <EXAMPLE>Les grimaces du clown amusent les enfants</EXAMPLE>
               </EXAMPLES>
               <SYNTAX>
                    <NP modifier="+genitive (Stimulus)" value="Attribute">
                         <SYNRESTRS/>
                    </NP>
                    <VERB/>
                    <NP value="Experiencer">
                         <SYNRESTRS/>
                    </NP>
               </SYNTAX>
               <SEMANTICS>cause(Stimulus, E) emotional_state(result(E), Emotion, Experiencer)</SEMANTICS>
          </FRAME>
          <FRAME>
               <DESCRIPTION primary="NP V NP PP.attribute" syntax="Stimulus V Experiencer {par/avec} Attribute&lt;+genitive (Stimulus)&gt;"/>
