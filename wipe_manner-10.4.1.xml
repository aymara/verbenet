<?xml version="1.0" encoding="UTF-8"?>
<VNCLASS ID="wipe_manner-10.4.1">
     <MEMBERS>
          <MEMBER name="presser"/>
          <MEMBER name="décaper"/>
          <MEMBER name="draguer"/>
          <MEMBER name="effacer"/>
          <MEMBER name="ramoner"/>
          <MEMBER name="récurer"/>
          <MEMBER name="laver"/>
          <MEMBER name="lessiver"/>
          <MEMBER name="rincer"/>
          <MEMBER name="extraire"/>
     </MEMBERS>
     <THEMROLES>
          <THEMROLE type="Agent">
               <SELRESTR Value="+" type="int_control"/>
          </THEMROLE>
          <THEMROLE type="Theme">
               <SELRESTRS logic="and">
                    <SELRESTR Value="-" type="animate"/>
                    <SELRESTR Value="+" type="concrete"/>
               </SELRESTRS>
          </THEMROLE>
          <THEMROLE type="Source">
               <SELRESTR Value="+" type="location"/>
          </THEMROLE>
     </THEMROLES>
     <FRAMES>
          <FRAME>
               <DESCRIPTION primary="NP V NP PP.source"/>
               <EXAMPLES>
                    <EXAMPLE>Luc a nettoyé les traces de doigt du comptoir.</EXAMPLE>
               </EXAMPLES>
               <SYNTAX>Agent V Theme {{de}} Source</SYNTAX>
               <SEMANTICS>cause(Agent, E) location(start(E), Theme, Source) not(location(end(E), Theme, Source))</SEMANTICS>
          </FRAME>
          <FRAME>
               <DESCRIPTION primary="NP V NP PP.theme"/>
               <EXAMPLES>
                    <EXAMPLE>Luc a nettoyé le comptoir des miettes.</EXAMPLE>
               </EXAMPLES>
               <SYNTAX>
                    <NP value="Agent">
                         <SYNRESTRS/>
                    </NP>
                    <VERB/>
                    <NP value="Source">
                         <SYNRESTRS/>
                    </NP>
                    <PREP>
                         <SELRESTRS>
                              <SELRESTR Value="de"/>
                         </SELRESTRS>
                    </PREP>
                    <NP value="Theme">
                         <SYNRESTRS/>
                    </NP>
               </SYNTAX>
               <SEMANTICS>cause(Agent, E) location(start(E), Theme, Source) not(location(end(E), Theme, Source))</SEMANTICS>
          </FRAME>
     </FRAMES>
</VNCLASS>
