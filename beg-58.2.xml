<?xml version="1.0" encoding="UTF-8"?>
<VNCLASS ID="beg-58.2">
     <MEMBERS>
          <MEMBER name="conjurer"/>
          <MEMBER name="implorer"/>
          <MEMBER name="presser"/>
          <MEMBER name="prier"/>
          <MEMBER name="requérir"/>
          <MEMBER name="supplier"/>
     </MEMBERS>
     <THEMROLES>
          <THEMROLE type="Agent">
               <SELRESTRS logic="or">
                    <SELRESTR Value="+" type="organization"/>
                    <SELRESTR Value="+" type="animate"/>
               </SELRESTRS>
          </THEMROLE>
          <THEMROLE type="Recipient">
               <SELRESTRS logic="or">
                    <SELRESTR Value="+" type="organization"/>
                    <SELRESTR Value="+" type="animate"/>
               </SELRESTRS>
          </THEMROLE>
          <THEMROLE type="Topic"/>
     </THEMROLES>
     <FRAMES>
          <FRAME>
               <DESCRIPTION primary="NP V NP de V-inf"/>
               <EXAMPLES>
                    <EXAMPLE>Luc a prié Léa de faire la vaiselle.</EXAMPLE>
               </EXAMPLES>
               <SYNTAX>
                    <NP value="Agent">
                         <SYNRESTRS/>
                    </NP>
                    <VERB/>
                    <NP value="Recipient">
                         <SYNRESTRS/>
                    </NP>
                    <VINF emptysubjectrole="Recipient" introduced_by="de" is_true_prep="true" value="Topic"/>
               </SYNTAX>
               <SEMANTICS>supplicate(during(E), Agent, Recipient, Topic)</SEMANTICS>
          </FRAME>
     </FRAMES>
     <SUBCLASSES>
          <VNSUBCLASS ID="58.2-1">
               <MEMBERS/>
               <THEMROLES/>
               <FRAMES>
                    <FRAME>
                         <DESCRIPTION primary="NP V de V-inf"/>
                         <EXAMPLES>
                              <EXAMPLE>Luc a imploré de partir.</EXAMPLE>
                         </EXAMPLES>
                         <SYNTAX>Agent V  Topic&lt;+de VAgent-inf&gt;</SYNTAX>
                         <SEMANTICS>supplicate(during(E), Agent, ?Recipient, Topic)</SEMANTICS>
                    </FRAME>
                    <FRAME>
                         <DESCRIPTION primary="NP V Qu Psubj"/>
                         <EXAMPLES>
                              <EXAMPLE>Luc a imploré que Marie fasse la vaisselle.</EXAMPLE>
                         </EXAMPLES>
                         <SYNTAX>
                              <NP value="Agent">
                                   <SYNRESTRS/>
                              </NP>
                              <VERB/>
                              <PSUBJ value="Topic"/>
                         </SYNTAX>
                         <SEMANTICS>supplicate(during(E), Agent, ?Recipient, Topic)</SEMANTICS>
                    </FRAME>
               </FRAMES>
          </VNSUBCLASS>
          <VNSUBCLASS ID="58.2-2">
               <MEMBERS/>
               <THEMROLES/>
               <FRAMES>
                    <FRAME>
                         <DESCRIPTION primary="NP V NP PP.topic"/>
                         <EXAMPLES>
                              <EXAMPLE>Luc a prié Marie (de ce) que leur fils fasse la vaisselle. </EXAMPLE>
                         </EXAMPLES>
                         <SYNTAX>
                              <NP value="Agent">
                                   <SYNRESTRS/>
                              </NP>
                              <VERB/>
                              <NP value="Recipient">
                                   <SYNRESTRS/>
                              </NP>
                              <PREP>
                                   <SELRESTRS>
                                        <SELRESTR Value="de"/>
                                   </SELRESTRS>
                              </PREP>
                              <NP modifier="+Qu Psubj" value="Topic">
                                   <SYNRESTRS/>
                              </NP>
                         </SYNTAX>
                         <SEMANTICS>supplicate(during(E), Agent, Recipient, Topic)</SEMANTICS>
                    </FRAME>
               </FRAMES>
          </VNSUBCLASS>
     </SUBCLASSES>
</VNCLASS>
