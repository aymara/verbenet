<?xml version="1.0" encoding="UTF-8"?>
<VNCLASS ID="estimate-34.2">
     <MEMBERS>
          <MEMBER name="calculer"/>
          <MEMBER name="compter"/>
          <MEMBER name="estimer"/>
          <MEMBER name="noter"/>
          <MEMBER name="juger"/>
          <MEMBER name="supputer"/>
          <MEMBER name="approximer"/>
          <MEMBER name="analyser"/>
          <MEMBER name="évaluer"/>
          <MEMBER name="jauger"/>
          <MEMBER name="diagnostiquer"/>
     </MEMBERS>
     <THEMROLES>
          <THEMROLE type="Agent">
               <SELRESTRS logic="or">
                    <SELRESTR Value="+" type="organization"/>
                    <SELRESTR Value="+" type="animate"/>
               </SELRESTRS>
          </THEMROLE>
          <THEMROLE type="Theme"/>
          <THEMROLE type="Value"/>
     </THEMROLES>
     <FRAMES>
          <FRAME>
               <DESCRIPTION primary="NP V NP"/>
               <EXAMPLES>
                    <EXAMPLE>L'inspecteur a estimé la probabilité que la maison s'écroule.</EXAMPLE>
               </EXAMPLES>
               <SYNTAX>
                    <NP value="Agent">
                         <SYNRESTRS/>
                    </NP>
                    <VERB/>
                    <NP value="Theme">
                         <SYNRESTRS/>
                    </NP>
               </SYNTAX>
               <SEMANTICS>assess(during(E), Agent, Theme, ?Value)</SEMANTICS>
          </FRAME>
          <FRAME>
               <DESCRIPTION primary="NP V NP PP.value"/>
               <EXAMPLES>
                    <EXAMPLE>L'inspecteur a estimé la probabilité à 20%.</EXAMPLE>
               </EXAMPLES>
               <SYNTAX>
                    <NP value="Agent">
                         <SYNRESTRS/>
                    </NP>
                    <VERB/>
                    <NP value="Theme">
                         <SYNRESTRS/>
                    </NP>
                    <PREP>
                         <SELRESTRS>
                              <SELRESTR Value="à"/>
                         </SELRESTRS>
                    </PREP>
                    <NP value="Value">
                         <SYNRESTRS/>
                    </NP>
               </SYNTAX>
               <SEMANTICS>assess(during(E), Agent, Theme, Value)</SEMANTICS>
          </FRAME>
          <FRAME>
               <DESCRIPTION primary="NP V combien P"/>
               <EXAMPLES>
                    <EXAMPLE>L'inspecteur estime combien ça peut coûter.</EXAMPLE>
               </EXAMPLES>
               <SYNTAX>
                    <NP value="Agent">
                         <SYNRESTRS/>
                    </NP>
                    <VERB/>
                    <P introduced_by="combien" value="Value"/>
               </SYNTAX>
               <SEMANTICS>assess(during(E), Agent, ?Theme, Value)</SEMANTICS>
          </FRAME>
          <FRAME>
               <DESCRIPTION primary="NP V Qu Pind"/>
               <EXAMPLES>
                    <EXAMPLE>L'inspecteur estime que c'est bien probable.</EXAMPLE>
               </EXAMPLES>
               <SYNTAX>
                    <NP value="Agent">
                         <SYNRESTRS/>
                    </NP>
                    <VERB/>
                    <PIND value="Theme"/>
               </SYNTAX>
               <SEMANTICS>assess(during(E), Agent, Theme, ?Value)</SEMANTICS>
          </FRAME>
          <FRAME>
               <DESCRIPTION primary="NP V si P"/>
               <EXAMPLES>
                    <EXAMPLE>L'inspecteur estime si la maison va s'écrouler</EXAMPLE>
               </EXAMPLES>
               <SYNTAX>
                    <NP value="Agent">
                         <SYNRESTRS/>
                    </NP>
                    <VERB/>
                    <P introduced_by="si" value="Theme"/>
               </SYNTAX>
               <SEMANTICS>assess(during(E), Agent, Theme, ?Value)</SEMANTICS>
          </FRAME>
     </FRAMES>
</VNCLASS>
