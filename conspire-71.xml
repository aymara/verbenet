<?xml version="1.0" encoding="UTF-8"?>
<VNCLASS ID="conspire-71">
     <MEMBERS>
          <MEMBER name="collaborer"/>
          <MEMBER name="comploter"/>
          <MEMBER name="rebeller se"/>
          <MEMBER name="conspirer"/>
          <MEMBER name="riposter"/>
     </MEMBERS>
     <THEMROLES>
          <THEMROLE type="Agent">
               <SELRESTRS logic="or">
                    <SELRESTR Value="+" type="organization"/>
                    <SELRESTR Value="+" type="animate"/>
               </SELRESTRS>
          </THEMROLE>
          <THEMROLE type="Co-Agent">
               <SELRESTRS logic="or">
                    <SELRESTR Value="+" type="organization"/>
                    <SELRESTR Value="+" type="animate"/>
               </SELRESTRS>
          </THEMROLE>
          <THEMROLE type="Theme"/>
     </THEMROLES>
     <FRAMES>
          <FRAME>
               <DESCRIPTION primary="NP V PP.co-agent pour V0-inf"/>
               <EXAMPLES>
                    <EXAMPLE>Jean conspire avec Marie pour prendre le pouvoir</EXAMPLE>
               </EXAMPLES>
               <SYNTAX>Agent V {avec} Co_Agent {pour} Theme &lt;V0-inf&gt;</SYNTAX>
               <SEMANTICS>conspire(during(E), Agent_i, Agent_j, Theme)</SEMANTICS>
          </FRAME>
          <FRAME>
               <DESCRIPTION primary="NP V PP.co-agent PP.theme"/>
               <EXAMPLES>
                    <EXAMPLE>Jean conspire avec Marie contre Max</EXAMPLE>
               </EXAMPLES>
               <SYNTAX>
                    <NP value="Agent">
                         <SYNRESTRS/>
                    </NP>
                    <VERB/>
                    <PREP>
                         <SELRESTRS>
                              <SELRESTR Value="avec"/>
                         </SELRESTRS>
                    </PREP>
                    <NP value="Co-Agent">
                         <SYNRESTRS/>
                    </NP>
                    <PREP>
                         <SELRESTRS>
                              <SELRESTR Value="contre"/>
                         </SELRESTRS>
                    </PREP>
                    <NP value="Theme">
                         <SYNRESTRS/>
                    </NP>
               </SYNTAX>
               <SEMANTICS>conspire(during(E), Agent, Co-Agent, Theme)</SEMANTICS>
          </FRAME>
          <FRAME>
               <DESCRIPTION primary="NP V PP.theme"/>
               <EXAMPLES>
                    <EXAMPLE>Jean et Marie conspirent contre Max</EXAMPLE>
               </EXAMPLES>
               <SYNTAX>
                    <NP value="Agent">
                         <SYNRESTRS/>
                    </NP>
                    <VERB/>
                    <PREP>
                         <SELRESTRS>
                              <SELRESTR Value="contre"/>
                         </SELRESTRS>
                    </PREP>
                    <NP value="Theme">
                         <SYNRESTRS/>
                    </NP>
               </SYNTAX>
               <SEMANTICS>conspire(during(E), Agent, Theme)</SEMANTICS>
          </FRAME>
          <FRAME>
               <DESCRIPTION primary="NP V pour V0_INF"/>
               <EXAMPLES>
                    <EXAMPLE>Jean et Marie conspirent pour prendre le pouvoir</EXAMPLE>
               </EXAMPLES>
               <SYNTAX>Agent V {pour} Theme &lt;V0-inf&gt;</SYNTAX>
               <SEMANTICS>conspire(during(E), Agent, Theme)</SEMANTICS>
          </FRAME>
     </FRAMES>
</VNCLASS>
