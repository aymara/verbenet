                    <EXAMPLE>Max a aidé Luc à finir ce travail</EXAMPLE>
               </EXAMPLES>
               <SYNTAX>Agent V Beneficiary {à} Theme &lt;+V1-inf&gt;</SYNTAX>
               <SEMANTICS>help(during(E), Agent, Beneficiary, Theme)</SEMANTICS>
          </FRAME>
          <FRAME>
               <DESCRIPTION primary="NP V NP PP.theme"/>
               <EXAMPLES>
                    <EXAMPLE>Max aide Luc dans son travail</EXAMPLE>
               </EXAMPLES>
               <SYNTAX>Agent V Beneficiary {dans} Theme &lt;-V1-inf&gt;</SYNTAX>
               <SEMANTICS>help(during(E), Agent, Beneficiary, Theme)</SEMANTICS>
          </FRAME>
     </FRAMES>
