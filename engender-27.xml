<?xml version="1.0" encoding="UTF-8"?>
<VNCLASS ID="engender-27">
     <MEMBERS>
          <MEMBER name="causer"/>
          <MEMBER name="déterminer"/>
          <MEMBER name="engendrer"/>
          <MEMBER name="faire"/>
          <MEMBER name="produire"/>
          <MEMBER name="provoquer"/>
     </MEMBERS>
     <THEMROLES>
          <THEMROLE type="Cause">
               <SELRESTR Value="+" type="abstract"/>
          </THEMROLE>
          <THEMROLE type="Theme">
               <SELRESTR Value="+" type="abstract"/>
          </THEMROLE>
          <THEMROLE type="Predicate"/>
     </THEMROLES>
     <FRAMES>
          <FRAME>
               <DESCRIPTION primary="NP V NP"/>
               <EXAMPLES>
                    <EXAMPLE>L'inégalité engendre des conflits</EXAMPLE>
               </EXAMPLES>
               <SYNTAX>
                    <NP value="Cause">
                         <SYNRESTRS/>
                    </NP>
                    <VERB/>
                    <NP value="Theme">
                         <SYNRESTRS/>
                    </NP>
               </SYNTAX>
               <SEMANTICS>not(exist(start(E), Theme)) exist(result(E), Theme) nonagentive_cause(Cause, E)</SEMANTICS>
          </FRAME>
          <FRAME>
               <DESCRIPTION primary="NP V that S"/>
               <EXAMPLES>
                    <EXAMPLE>La crise a provoqué que le chomage augmente</EXAMPLE>
               </EXAMPLES>
               <SYNTAX>Cause V Theme  &lt;+que_comp&gt;</SYNTAX>
               <SEMANTICS>not(exist(start(E), Theme)) exist(result(E), Theme) nonagentive_cause(Cause, E)</SEMANTICS>
          </FRAME>
     </FRAMES>
     <SUBCLASSES>
          <VNSUBCLASS ID="27-1">
               <MEMBERS/>
               <THEMROLES/>
               <FRAMES>
                    <FRAME>
                         <DESCRIPTION primary="NP  V NP PP.bénéficiaire"/>
                         <EXAMPLES>
                              <EXAMPLE>La crise a provoqué une augmentation du chomage {pour chez) les personnes âgées</EXAMPLE>
                         </EXAMPLES>
                         <SYNTAX>Cause V Theme {pour chez} Bénéficiare</SYNTAX>
                         <SEMANTICS>not(exist(start(E), Theme)) exist(result(E), Theme) nonagentive_cause(Cause, E) subir (Theme Bénéficiaire)</SEMANTICS>
                    </FRAME>
               </FRAMES>
          </VNSUBCLASS>
     </SUBCLASSES>
</VNCLASS>
